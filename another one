        Console.Write("Podaj liczbę: ");
        string a = Console.ReadLine();

       
        if (int.TryParse(a, out int v))
        {
            
            if (Math.Abs(v) >= 100 && Math.Abs(v) <= 999 && v % 17 == 0)
            {
                Console.WriteLine("TAK");
            }
            else
            {
                Console.WriteLine("NIE");
            }
        }
        else
        {
            Console.WriteLine("NIEPRAWIDŁOWE DANE");
        }
    

